<div id="teste"></div>
<script>
const numeros = [40,100,1,5,25,10];
numeros.sort(function  (a,b){return b - a});
document.getElementById("teste").innerHTML = MaiorNumero(numeros);
function MaiorNumero(array){
    return Math.max.apply(null, array);
}

</script>
